<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <nav>
      <%- include ('../partials/welcome.ejs') %>
    </nav>
    <h1>Projects index page</h1>
    <div class="text">
    <%JSON.stringify(projectList)%>
      <br/>
        <% for(let i=0; i< projectList.length; i++){ %>
            <div class="card">
              <img class="images" src="<%=projectList[i].previewImage%>"></img><br/>
              <div class="name">
              <a class="nameLink" href="/portfolio/<%=projectList[i].id%>">
                <%=projectList[i].projectName %>
              </a>
              </div>
            </div>
            <br/>
        <%  } %>
    </div>
    <br />
    <nav>
      <a class="new" href="/portfolio/new">Create a new product</a>
  </body>
</html>
